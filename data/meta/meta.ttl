#World Development Indicators
wbld-dataset:world-development-indicators
    a qb:DataSet ;
    qb:structure wbld-dataset-world-development-indicators:structure ;
    rdfs:label "World Development Indicators dataset"@en ;
    dcterms:issued "2012-02-29T00:00:00Z"^^xsd:dateTime ;
    dcterms:source <http://api.worldbank.org/indicators?format=xml> ;
    dcterms:creator <http://csarven.ca/#i> ;
    dcterms:subject
        <http://purl.org/linked-data/sdmx/2009/subject#1.2>,
        <http://purl.org/linked-data/sdmx/2009/subject#1.3>,
        <http://purl.org/linked-data/sdmx/2009/subject#1.4>,
        <http://purl.org/linked-data/sdmx/2009/subject#1.6>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.4.1>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.4.2>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.4.3>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.4.4>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.4.6>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.5>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.7>,
        <http://purl.org/linked-data/sdmx/2009/subject#2.9>,
        <http://purl.org/linked-data/sdmx/2009/subject#3.1>,
        <http://purl.org/linked-data/sdmx/2009/subject#3.3.1>,
        <http://purl.org/linked-data/sdmx/2009/subject#3.3.2>,
        <http://purl.org/linked-data/sdmx/2009/subject#3.3.3>
    .

wbld-dataset-world-development-indicators:structure
    a qb:DataStructureDefinition ;
    qb:component
        [
            a qb:ComponentSpecification ;
            qb:dimension property:indicator ;
            qb:order "1"^^xsd:int
        ], [
            a qb:ComponentSpecification ;
            qb:dimension sdmx-dimension:refArea ;
            qb:order "2"^^xsd:int
        ], [
            a qb:ComponentSpecification ;
            qb:dimension sdmx-dimension:refPeriod ;
            qb:order "3"^^xsd:int
        ], [
            a qb:ComponentSpecification ;
            qb:measure qb:obsValue ;
            qb:order "4"^^xsd:int
        ]
    .


